<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <JsonLayout
                    complete="false"
                    compact="true"
                    eventEol="true"
                    stacktraceAsString="true"
                    objectMessageAsJsonObject="true"
                    properties="true">
            </JsonLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>